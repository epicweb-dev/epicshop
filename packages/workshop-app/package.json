    "express": "^4.18.2",
    "fkill": "^8.1.0",
    "framer-motion": "^10.12.21",
    "fs-extra": "^11.1.1",
    "get-port": "^7.0.0",
    "glob": "^10.3.3",
    "globby": "^13.2.2",
    "isbot": "^3.6.12",
    "lodash.escape": "^4.0.1",
    "lru-cache": "^10.0.0",
    "md5-hex": "^4.0.0",
