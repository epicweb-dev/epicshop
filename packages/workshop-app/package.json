{
  "name": "@kentcdodds/workshop-app",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "kcdshop": "./bin/kcdshop.js"
  },
  "files": [
    "start.js",
    "build",
    "public"
  ],
  "type": "module",
  "exports": {
    "./apps.server": {
      "types": "./build/utils/apps.server.d.ts",
      "default": "./build/utils/apps.server.js"
    },
    "./iframe-sync": {
      "types": "./build/utils/iframe-sync.d.ts",
      "default": "./build/utils/iframe-sync.js"
    },
    "./playwright": {
      "types": "./build/utils/playwright.d.ts",
      "default": "./build/utils/playwright.js"
    },
    "./process-manager.server": {
      "types": "./build/utils/process-manager.server.d.ts",
      "default": "./build/utils/process-manager.server.js"
    },
    "./test": {
      "types": "./build/utils/test.d.ts",
      "default": "./build/utils/test.js"
    }
  },
  "scripts": {
    "build": "run-p --print-label --print-name build:*",
    "build:remix": "remix build --sourcemap",
    "build:server": "tsx ./other/build-dir.ts server",
    "build:utils": "tsx ./other/build-dir.ts utils",
    "build:decs": "tsc --build ./utils/tsconfig.json",
    "dev": "cross-env NODE_ENV=development FORCE_COLOR=true run-p --print-label --print-name dev:*",
    "dev:remix": "remix dev -c \"npm run start:server\" --no-restart",
    "start:server": "tsx watch --clear-screen=false --ignore \"app/**\" --ignore \"build/**\" --ignore \"utils/**\" --ignore \"node_modules/**\" --inspect ./start.js",
    "dev:utils": "tsx --watch --no-warnings --watch-preserve-output --watch-path ./utils ./other/build-dir.ts utils",
    "dev:decs": "npm run build:decs --silent -- --preserveWatchOutput --watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./start.js",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "dependencies": {
    "@kentcdodds/md-temp": "^3.2.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@remix-run/css-bundle": "^1.16.1",
    "@remix-run/express": "^1.16.1",
    "@remix-run/node": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@remix-run/serve": "^1.16.1",
    "@remix-run/server-runtime": "^1.16.1",
    "@types/chai": "^4.3.5",
    "@types/chai-dom": "^1.11.0",
    "address": "^1.2.2",
    "ansi-to-html": "^0.7.2",
    "baffle-react": "^0.0.2",
    "cachified": "^3.5.4",
    "chai": "^4.3.7",
    "chai-dom": "^1.11.0",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "close-with-grace": "^1.2.0",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.1.3",
    "esbuild": "^0.17.19",
    "execa": "^7.1.1",
    "express": "^4.18.2",
    "fkill": "^8.1.0",
    "framer-motion": "^10.12.16",
    "fs-extra": "^11.1.1",
    "get-port": "^6.1.2",
    "glob": "^10.2.6",
    "globby": "^13.1.4",
    "isbot": "^3.6.10",
    "lodash.escape": "^4.0.1",
    "lru-cache": "^9.1.2",
    "md5-hex": "^4.0.0",
    "mdx-bundler": "^9.2.1",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "p-map": "^6.0.0",
    "parse-git-diff": "^0.0.10",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-autolink-headings": "^7.0.1",
    "remark-emoji": "^3.1.1",
    "remark-gfm": "^3.0.1",
    "remix-flat-routes": "^0.5.8",
    "remix-utils": "^6.3.0",
    "shell-quote": "^1.8.1",
    "shiki": "^0.14.2",
    "spin-delay": "^1.2.0",
    "tailwindcss-radix": "^2.8.0",
    "tiny-invariant": "^1.3.1",
    "tsx": "^3.12.7",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "wait-on": "^7.0.1",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/compression": "^1.7.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/eslint": "^8.40.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.escape": "^4.0.7",
    "@types/lru-cache": "^7.10.10",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/shell-quote": "^1.7.1",
    "@types/wait-on": "^5.3.1",
    "@types/ws": "^8.5.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "18"
  }
}
