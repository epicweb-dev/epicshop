{
  "name": "@kentcdodds/workshop-app",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "kcdshop": "./bin/kcdshop.js"
  },
  "files": [
    "apps.server.d.ts",
    "apps.server.js",
    "start.js",
    "iframe-sync.d.ts",
    "iframe-sync.js",
    "playwright.d.ts",
    "playwright.js",
    "test.d.ts",
    "test.js",
    "build",
    "public"
  ],
  "scripts": {
    "build": "run-s --print-label --print-name build:*",
    "build:remix": "remix build --sourcemap",
    "build:server": "tsx ./other/build-dir.ts server",
    "build:utils": "tsx ./other/build-dir.ts utils",
    "build:decs": "tsc --build ./utils/tsconfig.json",
    "dev": "cross-env NODE_ENV=development FORCE_COLOR=true run-p --print-label --print-name dev:*",
    "dev:remix": "remix watch",
    "dev:server": "tsx watch --clear-screen=false --ignore \"app/**\" --ignore \"build/**\" --ignore \"node_modules/**\" ./start.js",
    "dev:utils": "tsx --watch --no-warnings --watch-preserve-output --watch-path ./utils ./other/build-dir.ts utils",
    "dev:decs": "npm run build:decs --silent -- --preserveWatchOutput --watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./start.js",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "dependencies": {
    "@kentcdodds/md-temp": "^3.1.1",
    "@radix-ui/react-tabs": "^1.0.2",
    "@remix-run/express": "^1.13.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/server-runtime": "^1.13.0",
    "address": "^1.2.2",
    "ansi-to-html": "^0.7.2",
    "cachified": "^3.1.0",
    "chai": "^4.3.7",
    "chai-dom": "^1.11.0",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "close-with-grace": "^1.1.0",
    "clsx": "^1.2.1",
    "common-ancestor-path": "^1.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.8",
    "execa": "^7.0.0",
    "express": "^4.18.2",
    "fkill": "^8.1.0",
    "fs-extra": "^11.1.0",
    "get-port": "^6.1.2",
    "glob": "^8.1.0",
    "globby": "^13.1.3",
    "isbot": "^3.6.6",
    "lodash.escape": "^4.0.1",
    "lru-cache": "^7.14.1",
    "match-sorter": "^6.3.1",
    "md5-hex": "^4.0.0",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-mdx": "^2.0.1",
    "mdast-util-to-markdown": "^1.5.0",
    "mdx-bundler": "^9.2.1",
    "micromark-extension-mdxjs": "^1.0.0",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "parse-git-diff": "^0.0.7",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-autolink-headings": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remix-flat-routes": "^0.5.7",
    "remix-utils": "^6.0.0",
    "shell-quote": "^1.8.0",
    "shiki": "^0.14.1",
    "tailwindcss-radix": "^2.7.0",
    "tiny-invariant": "^1.3.1",
    "tsx": "^3.12.3",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "wait-on": "^7.0.1",
    "ws": "^8.12.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/chai": "^4.3.4",
    "@types/chai-dom": "^1.11.0",
    "@types/common-ancestor-path": "^1.0.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.21.1",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.escape": "^4.0.7",
    "@types/lru-cache": "^7.10.10",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.13.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/shell-quote": "^1.7.1",
    "@types/wait-on": "^5.3.1",
    "@types/ws": "^8.5.4",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "18"
  }
}
