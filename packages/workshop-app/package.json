    "get-port": "^7.0.0",
    "glob": "^10.3.4",
    "globby": "^13.2.2",
    "isbot": "^3.6.13",
    "lodash.escape": "^4.0.1",
    "lru-cache": "^10.0.1",
    "md5-hex": "^4.0.0",
    "mdx-bundler": "^9.2.1",
