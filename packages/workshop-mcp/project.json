{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"name": "@epic-web/workshop-mcp",
	"projectType": "library",
	"sourceRoot": "packages/workshop-mcp/src",
	"implicitDependencies": ["@epic-web/workshop-utils"],
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "packages/workshop-mcp",
				"command": "npm run build"
			}
		},
		"typecheck": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "packages/workshop-mcp",
				"command": "npm run typecheck"
			}
		}
	}
}
