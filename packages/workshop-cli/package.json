{
  "name": "epicshop",
  "version": "0.0.0-semantically-released",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": "./dist/cli.js",
  "zshy": {
    "cjs": false,
    "exports": {
      "./package.json": "./package.json",
      "./warm": "./src/commands/warm.ts",
      "./start": "./src/commands/start.ts",
      "./update": "./src/commands/update.ts",
      "./workshops": "./src/commands/workshops.ts"
    },
    "bin": "./src/cli.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    "./warm": {
      "types": "./dist/commands/warm.d.ts",
      "import": "./dist/commands/warm.js"
    },
    "./start": {
      "types": "./dist/commands/start.d.ts",
      "import": "./dist/commands/start.js"
    },
    "./update": {
      "types": "./dist/commands/update.d.ts",
      "import": "./dist/commands/update.js"
    },
    "./workshops": {
      "types": "./dist/commands/workshops.d.ts",
      "import": "./dist/commands/workshops.js"
    }
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "build": "zshy",
    "build:watch": "nx watch --projects=epicshop -- nx run \\$NX_PROJECT_NAME:build"
  },
  "dependencies": {
    "@epic-web/workshop-utils": "file:../workshop-utils",
    "@inquirer/prompts": "^7.5.1",
    "chalk": "^5.6.2",
    "close-with-grace": "^2.3.0",
    "get-port": "^7.1.0",
    "match-sorter": "^8.2.0",
    "openid-client": "^6.8.1",
    "open": "^10.2.0",
    "ora": "^8.1.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@epic-web/config": "^1.21.3",
    "@types/node": "^24.10.0",
    "@types/yargs": "^17.0.34",
    "zshy": "^0.3.0",
    "vitest": "^4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicweb-dev/epicshop.git",
    "directory": "packages/workshop-cli"
  }
}
