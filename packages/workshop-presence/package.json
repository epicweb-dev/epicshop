{
  "name": "@epic-web/workshop-presence",
  "version": "0.0.0-semantically-released",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "zshy",
    "dev": "partykit dev",
    "deploy": "partykit deploy"
  },
  "dependencies": {
    "@epic-web/workshop-utils": "file:../workshop-utils",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "partykit": "0.0.115",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "zshy": "^0.7.0"
  },
  "files": [
    "dist"
  ],
  "zshy": {
    "cjs": false,
    "conditions": {
      "import": "esm"
    },
    "exports": {
      "./package.json": "./package.json",
      "./presence.server": "./src/presence.server.ts",
      "./presence": "./src/presence.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicweb-dev/epicshop.git",
    "directory": "packages/workshop-presence"
  },
  "exports": {
    "./package.json": "./package.json",
    "./presence.server": {
      "import": "./dist/presence.server.js",
      "types": "./dist/presence.server.d.ts",
      "default": "./dist/presence.server.js"
    },
    "./presence": {
      "import": "./dist/presence.js",
      "types": "./dist/presence.d.ts",
      "default": "./dist/presence.js"
    }
  }
}
