{
  "name": "example",
  "private": true,
  "kcd-workshop": {
    "title": "Web App Fundamentals ðŸ”­",
    "root": true
  },
  "scripts": {
    "build": "npm run build --if-present --workspaces",
    "start": "kcdshop start",
    "dev": "kcdshop start",
    "test": "cross-env NODE_ENV=development playwright test",
    "postinstall": "npx playwright install chromium --with-deps",
